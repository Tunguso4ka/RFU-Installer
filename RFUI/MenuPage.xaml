<Page x:Class="RFUI.MenuPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:RFUI"
      mc:Ignorable="d" 
      d:DesignHeight="720" d:DesignWidth="200"
      Title="MenuPage">

    <StackPanel>
        <Button Content="RFUpdater" Width="200" Name="App" Click="Button_Click" ToolTip="RFUpdater" Tag="App"/>
        <Button Content="" Width="200"/>
        <Button Content="Settings" Width="200" Name="Settings" Click="Button_Click" ToolTip="Settings" Tag="Settings"/>
        <Button Content="Exit" Width="200" Name="Exit" Click="Button_Click" ToolTip="Exit" Tag="Exit"/>
    </StackPanel>
</Page>
