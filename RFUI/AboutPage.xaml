<Page x:Class="RFUI.AboutPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:RFUI"
      mc:Ignorable="d" 
      d:DesignHeight="720" d:DesignWidth="1280"
      Title="AboutPage">

    <Grid>
        <StackPanel Orientation="Horizontal" HorizontalAlignment="Center" VerticalAlignment="Top" Height="50">
            <Image Source="/Resources/rfuilogo.png" SnapsToDevicePixels="False"/>
            <Image Source="/Resources/rfulogo0525.png" SnapsToDevicePixels="True"/>
        </StackPanel>
        <TextBox Margin="0,50,0,0" Background="Transparent" BorderBrush="Transparent" Name="AboutTextBox" TextAlignment="Center" FontSize="24" Text="Test"/>
    </Grid>
</Page>
